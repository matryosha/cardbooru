{
  "fetchConfiguration": {
    "postLimit": 100,
    "activeSite": "Danbooru",
    "ratingConfiguration": {
      "safe": "true",
      "explicit": "true",
      "questionable":  "true" 
    },
    "globbingConfiguration": {
      "limit": "<limit>",
      "pageNumber": "<page-number>",
      "tags": "<tags>"
    },
    "danbooruUrlConfiguration": {
      "baseUrl": "https://danbooru.donmai.us",
      "postsUrl": "/posts.json?limit=<limit>&page=<page-number><tags>"
    },
    "safebooruUrlConfiguration": {
      "baseUrl": "http://safebooru.org",
      "postsUrl": "/index.php?page=dapi&s=post&q=index&limit=<limit>&pid=<page-number>&json=1"
    }
  },
  "cachePath": "cache/image/",
  "imageCaching": "false"
}