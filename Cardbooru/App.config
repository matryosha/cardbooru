<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="Cardbooru.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <userSettings>
        <Cardbooru.Properties.Settings>
            <setting name="CurrentSite" serializeAs="String">
                <value />
            </setting>
            <setting name="DanbooruUrl" serializeAs="String">
                <value>https://danbooru.donmai.us</value>
            </setting>
            <setting name="SafeBooruUrl" serializeAs="String">
                <value>http://safebooru.org</value>
            </setting>
            <setting name="DanbooruPostsUrl" serializeAs="String">
                <value>/posts.json?limit=*&amp;page=*/</value>
            </setting>
            <setting name="SafeBooruPostsUrl" serializeAs="String">
                <value>/index.php?page=dapi&amp;s=post&amp;q=index&amp;limit=*&amp;pid=*&amp;json=1</value>
            </setting>
            <setting name="NumberOfPicPerRequest" serializeAs="String">
                <value>500</value>
            </setting>
            <setting name="SafeCheck" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="QuestionableCheck" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="ExplicitCheck" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="UndefinedCheck" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="GelbooruSiteUrl" serializeAs="String">
                <value>https://gelbooru.com</value>
            </setting>
            <setting name="GelbooruPostsUrl" serializeAs="String">
                <value>/index.php?page=dapi&amp;s=post&amp;q=index&amp;limit=*&amp;pid=*&amp;json=1</value>
            </setting>
            <setting name="PathToCacheFolder" serializeAs="String">
                <value>cache/image/</value>
            </setting>
            <setting name="BlackListTags" serializeAs="Xml">
                <value>
                    <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xmlns:xsd="http://www.w3.org/2001/XMLSchema" />
                </value>
            </setting>
        </Cardbooru.Properties.Settings>
    </userSettings>
</configuration>
